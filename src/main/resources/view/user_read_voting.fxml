<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.text.Font?>
<?import javafx.scene.effect.InnerShadow?>
<GridPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.housemanagementsystem.votings.VotingController">

    <Button onAction="#onGoBackClick" style="-fx-background-color: ORANGE;" text="Back" GridPane.columnIndex="0" GridPane.rowIndex="10">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
        <effect>
            <InnerShadow />
        </effect></Button>

    <children>
        <TableView fx:id="userReadVotingTable" layoutX="35.0" layoutY="28.0" prefHeight="600.0" prefWidth="800.0">
            <columns>
                <TableColumn fx:id="votingIDCol" prefWidth="90.0" text="ID" />
                <TableColumn fx:id="votingStatusCol" prefWidth="90.0" text="Status" />
                <TableColumn fx:id="votingTitleCol" prefWidth="90.0" text="Title" />
                <TableColumn fx:id="votingAnswerCol" prefWidth="90.0" text="Answers" />
                <TableColumn fx:id="votingSubmitATCol" prefWidth="90.0" text="Submit At" />
                <TableColumn fx:id="votingUserIDCol" prefWidth="90.0" text="User ID" />
                <TableColumn fx:id="votingApartmentIDCol" prefWidth="90.0" text="Apartment ID" />
            </columns>
        </TableView>
    </children>
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>

</GridPane>